{
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.enableMessageDialog": true,
  "dialog.floatBorderHighlight": "FloatBorder",
  "suggest.floatConfig": {
    "border": true,
    "winblend": 15,
    "borderhighlight": "FloatBorder"
  },
  "hover.floatConfig": {
    "border": true,
    "winblend": 15,
    "borderhighlight": "FloatBorder"
  },
  "signature.floatConfig": {
    "border": true,
    "winblend": 15,
    "borderhighlight": "FloatBorder"
  },
  "diagnostic.floatConfig": {
    "border": true,
    "winblend": 15,
    "borderhighlight": "FloatBorder"
  },
  "Lua.runtime.version": "LuaJIT",
  "sumneko-lua.enableNvimLuaDev": true,
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.imports.granularity.group": "module",
  "rust-analyzer.updates.channel": "stable",
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "inlayHint.enable": false,
  "rust-analyzer.disableProgressNotifications": true,
  "clangd.arguments": [
    "--fallback-style", "{BasedOnStyle: webkit, IndentWidth: 2}",
    "--clang-tidy", "--clang-tidy-checks=*",
    "--cross-file-rename",
    "--index"
  ],
  "clangd.disableProgressNotifications": true,
  "yaml.format.enable": true,
  "python.formatting.provider": "black",
  "pyright.disableProgressNotifications": true
}
